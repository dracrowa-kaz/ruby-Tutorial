<table border ="1"> 
	<tr > 
		<th > ISBN コード </th >
		<th > 書名 </th >
		<th > 価格 </th > 
		<th > 刊行 日 </th >
		<th > CD-ROM </th > 
	</tr > 
		<% @books.each do | book | %> 
			<tr > 
				<td >
					<%= book.isbn %>
				</td > 
					<td >
						<%= book.title %>
					</td > 
					<td>
						<%= book.price %>円 
					</td > 
					<td>
						<%= book.published %>
					</td> 
					<td>
						<%= book.cd %>
					</td > 
				</tr > 
		<% end %>
</table>
